# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  firebase_id: String!
  name: String!
  description: String!
  group_id: String!
}

type Query {
  users: [User!]!
}

input NewUser {
  firebase_id: String!
  name: String!
  description: String!
  group_id: String!
}

type Mutation {
  createUser(input: NewUser!): User!
}
